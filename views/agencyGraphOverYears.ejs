<html>
  <head>

<% include header.ejs %>
<script type="text/javascript" src="http://www.amcharts.com/lib/3/amcharts.js"></script>
<script type="text/javascript" src="http://www.amcharts.com/lib/3/serial.js"></script>
<script type="text/javascript" src="http://www.amcharts.com/lib/3/themes/none.js"></script>
  	<script type="text/javascript">
  //	alert("hi");
  //  var test = <%= JSON.stringify(output1) %>;
  // alert(<%= output1[6]%>);
   
  
   var chart = AmCharts.makeChart("chartdiv", {
	    "theme": "none",
	    "type": "serial",
	    "dataProvider": [{
	        "Year": "2007",
	        "NASA":  <%= output1[0]%>,
	        "NSF": <%= output1[7]%>
	    }, {
	    	"Year": "2008",
	        "NASA": <%= output1[1]%>,
	        "NSF": <%= output1[8]%>
	    }, {
	    	"Year": "2009",
	        "NASA": <%= output1[2]%>,
	        "NSF": <%= output1[9]%>
	    }, {
	    	"Year": "2010",
	        "NASA": <%= output1[3]%>,
	        "NSF": <%= output1[10]%>
	    }, {
	    	"Year": "2011",
	        "NASA": <%= output1[4]%>,
	        "NSF": <%= output1[11]%>
	    }, {
	    	"Year": "2012",
	        "NASA": <%= output1[5]%>,
	        "NSF": <%= output1[12]%>
	    }, {
	    	"Year": "2013",
	        "NASA": <%= output1[6]%>,
	        "NSF": <%= output1[13]%>
	    }],
	    "startDuration": 1,
	    "graphs": [{
	        "balloonText": "Awards given in [[category]] by NASA: <b>[[value]]</b>",
	        "fillAlphas": 0.9,
	        "lineAlpha": 0.2,
	        "title": "NASA",
	        "type": "column",
	        "valueField": "NASA"
	    }, {
	        "balloonText": "Awards given in [[category]] by NSF: <b>[[value]]</b>",
	        "fillAlphas": 0.9,
	        "lineAlpha": 0.2,
	        "title": "NSF",
	        "type": "column",
	        "valueField": "NSF"
	    }],
	    "plotAreaFillAlphas": 0.1,
	    "depth3D": 60,
	    "angle": 30,
	    "categoryField": "Year",
	    "categoryAxis": {
	        "gridPosition": "start"
	    },
		"exportConfig":{
			"menuTop":"20px",
	        "menuRight":"20px",
	        "menuItems": [{
	        "icon": '/lib/3/images/export.png',
	        "format": 'png'	  
	        }]  
	    }
	});
    
    </script>
  </head>
  <body>
    <div id="chartdiv" style="background-color:white"></div>
    <% include footer.ejs %>
  </body>
</html>